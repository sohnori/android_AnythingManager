<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"    
    android:id="@+id/actp10"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="#505050"
    tools:context="com.daeeun.sohnori.anythingmanager.MyProcess10Activity" >    	
    <TextView
        android:id="@+id/act10txt" 
        android:layout_width="match_parent" 
        android:layout_height="wrap_content" 
        android:text="메인에서 호출한 서브입니다." 
        android:textSize="20sp" 
        android:textColor="#00ff00"             
        />
    <TextView
        android:id="@+id/act10txt2" 
        android:layout_width="match_parent" 
        android:layout_height="wrap_content" 
        android:text="메인에서 호출한 서브입니다." 
        android:textSize="20sp" 
        android:textColor="#00ff00"             
        />
    <Button	    
        android:id="@+id/act10btnhome"	    
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" 
        android:onClick="mOnClick"
        android:text="@string/mainpage"
        />
	    
    
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="wrap_content"
    	android:layout_height="wrap_content" 
        >
        <Button
	        android:id="@+id/act10btntype"	        
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content" 
	        android:onClick="mOnClick"
	        android:text="타입"
	        />
        <Button
	        android:id="@+id/act10btnmodel"	        
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content" 
	        android:onClick="mOnClick"
	        android:text="모델"
	        />
        <Button            
	        android:id="@+id/act10btnck"	        
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content" 
	        android:onClick="mOnClick"
	        android:text="체크메뉴"
	        />
    </LinearLayout>
    <Button 
        android:id="@+id/act10btnsptest"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="mOnClick"
        android:text="speed test"
        />
    <TextView 
        android:id="@+id/act10txtresult"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Result"
        />
        		       
</LinearLayout>